#include <stdio.h>
#include <stdlib.h>
typedef struct Node
{
	int val;
	struct Node *next;
}Node;

Node *list(Node *p)
{
	Node *q, *head;
	int i, n, a;
	scanf("%d", &n);
	head = NULL;
	for(i = 1; i<=n; i++){
		scanf("%d", &a);
		p = (Node*)malloc(sizeof(Node));
		p->val = a;
		p->next = NULL;
		if(head==NULL)
			head = p;
		else
			q->next = p;
		q = p;
	}
	return head;
}


 int main()
 {
	Node list1, list2, *save, *p;
	save = (Node*)malloc(sizeof(Node));
	save->next = NULL;
	p = (Node*)malloc(sizeof(Node));
	save = list(Node *p);
	
	 
	
	
  return 0;
}
  
  
  

